{
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "presets": ["env", "react", "stage-0"],
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  },
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./app"],
        "modules": ["app"]
      }
    ]
  ]
}
